{
  "manifest_version": 2,
  "default_locale": "zh_TW",
  "name": "__MSG_appName__",
  "short_name": "Scam Web and Content Farm Critic",
  "description": "__MSG_appDesc__",
  "version": "3.2",

  "browser_action": {
    "default_icon": "image/icon.png"
  },
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "identity",
    "tabs",
    "https://ajax.googleapis.com/",
    "http://*.facebook.com/*",
    "webRequest",
    "webRequestBlocking",
