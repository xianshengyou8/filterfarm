  "oauth2": {
    "client_id": "817781349645-k283oikrmcn38vkf7b8ltsrqdjai6p04.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.googleapis.com/auth/userinfo.profile"
    ]
  },
  "key": ""
}